<resources>

    <style name="CloatherToolbar" parent="Widget.MaterialComponents.Toolbar">
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="titleTextColor">@android:color/white</item>
        <item name="contentInsetStartWithNavigation">72dp</item>
    </style>

    <style name="CloatherRadioButton" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:fontFamily">@font/rubik_regular</item>
    </style>

    <style name="CloatherRadioButton.GenderRadioButton">
        <item name="android:textColor">@color/text_secondary</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="CloatherTextView" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/rubik_regular</item>
    </style>

</resources>
